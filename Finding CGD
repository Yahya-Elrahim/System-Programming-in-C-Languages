#include <stdio.h>
#include <stdlib.h>

int main()
{

    // 98 , 56
    // 98 = 1*56 + 42   ; 98-56=42
    // 42 = 1*28 + 14   ; 56-42=28
   //  28 = 14 + 14     ; 42-28=14
   //  14 = 14- 0       ; 28-14=14
   //                   ; 14-14=0
    int n=1,f_number=9702,s_number=945;
     printf("Enter the first number : ");
     scanf("%i",&f_number);
     printf("Enter the second number : ");
     scanf("%i",&s_number);

    while(s_number!=0)
    {
       if(f_number > s_number)
       {
            f_number=f_number-s_number;

       }
        else
        {
            s_number=s_number-f_number;
        }


    }
    printf("en buyuk ortan boleni : %i",f_number);
    return 0;
}
